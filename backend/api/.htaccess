<IfModule mod_rewrite.c>
    RewriteEngine On

    SetEnvIf Authorization "(.*)" HTTP_AUTHORIZATION=$1

    # Do not apply the rewrite rule to the /dev/ directory or its contents
    RewriteCond %{REQUEST_URI} !^/e_order/backend/api/dev/

    # If the request is not for an existing file or directory,
    # route it through index.php (for production pretty URLs).
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ prod/index.php/$1 [L]

</IfModule>
